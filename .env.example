# AWS設定
AWS_REGION=ap-northeast-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# DynamoDB設定
DYNAMODB_TABLE_NAME=my-nishikoyama-events
PUSH_SUBSCRIPTIONS_TABLE=my-nishikoyama-push-subscriptions

# スクレイピング設定
SCRAPING_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36
SCRAPING_INTERVAL=0 9 * * * # 毎日9時に実行

# Web Push通知設定
VAPID_EMAIL=your_email@example.com
VAPID_PUBLIC_KEY=your_vapid_public_key
VAPID_PRIVATE_KEY=your_vapid_private_key

# LINE Notify設定（2025年3月31日サービス終了予定）
LINE_NOTIFY_ENABLED=false
LINE_NOTIFY_TOKEN=your_line_notify_token

# 開発環境
NODE_ENV=development
PORT=3000

# フロントエンドURL
FRONTEND_URL=https://my-nishikoyama-frontend.vercel.app
